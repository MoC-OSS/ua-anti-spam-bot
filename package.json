{
  "name": "ua-anti-spam-bot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:bot": "nodemon ./src/bot.js --max_old_space_size=2048",
    "start:bot:prod": "node ./src/bot.js",
    "start:userbot:prod": "node ./src/userbot",
    "start:server": "nodemon ./src/express.js --max_old_space_size=2048",
    "start:server:prod": "node ./src/express.js",
    "start:server:cluster": "pm2 start ./src/express.js -i 3 --watch || echo '*** Install PM2 globally'",
    "prepare": "husky install",
    "lint": "npm-run-all --parallel lint:js",
    "lint:branch-name": "branch-name-lint ./.branchlintrc.json",
    "lint:commit-name": "commitlint --edit",
    "track-time": "smile-track",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@grammyjs/menu": "^1.0.4",
    "@grammyjs/parse-mode": "^1.1.2",
    "@grammyjs/router": "^1.1.1",
    "@grammyjs/transformer-throttler": "^1.1.1",
    "@keyv/sqlite": "^3.0.1",
    "@mtproto/core": "6.2.0",
    "@tensorflow/tfjs": "^3.15.0",
    "@tensorflow/tfjs-node": "^3.15.0",
    "axios": "^0.26.1",
    "contains-emoji": "^1.1.0",
    "cyrillic-to-translit-js": "^3.1.0",
    "diff": "^5.0.0",
    "express": "^4.17.3",
    "fuse.js": "^6.5.3",
    "grammy": "^1.7.1",
    "grapheme-splitter": "^1.0.4",
    "husky": "^7.0.4",
    "keyv": "^4.2.1",
    "lodash.get": "^4.4.2",
    "queue-promise": "^2.2.1",
    "redis": "^4.0.6",
    "typed-dotenv": "^10.0.1",
    "typegram": "^3.8.0",
    "ukrainian-ml-optimizer": "github:DrSmile444/ukrainian-ml-optimizer"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "branch-name-lint": "^2.1.1",
    "commitlint": "^16.2.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "smile-track": "^0.17.0"
  }
}
