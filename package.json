{
  "name": "ua-anti-spam-bot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:bot": "nodemon ./src/bot.js --ignore ./telegraf-session.json --ignore ./last-ctx.json --max_old_space_size=2048",
    "start:bot:prod": "node ./src/bot.js",
    "start:server": "nodemon ./src/express.js --ignore ./telegraf-session.json --ignore ./last-ctx.json --max_old_space_size=2048",
    "start:server:prod": "node ./src/express.js",
    "start:server:cluster": "pm2 start ./src/express.js -i 3 --watch",
    "prepare": "husky install",
    "lint": "npm-run-all --parallel lint:js",
    "lint:branch-name": "branch-name-lint ./.branchlintrc.json",
    "lint:commit-name": "commitlint --edit",
    "track-time": "smile-track",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@grammyjs/menu": "^1.0.4",
    "@grammyjs/parse-mode": "^1.0.0",
    "@keyv/sqlite": "^3.0.0",
    "axios": "^0.26.0",
    "contains-emoji": "^1.1.0",
    "cyrillic-to-translit-js": "^3.1.0",
    "express": "^4.17.3",
    "fuse.js": "^6.5.3",
    "grammy": "^1.7.0",
    "grapheme-splitter": "^1.0.4",
    "husky": "^7.0.2",
    "keyv": "^4.1.1",
    "lodash.get": "^4.4.2",
    "telegraf-session-local": "^2.1.0",
    "typed-dotenv": "^10.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "branch-name-lint": "^2.1.1",
    "commitlint": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.2.0",
    "prettier": "^2.3.1",
    "smile-track": "^0.17.0"
  }
}
