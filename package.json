{
  "name": "ua-anti-spam-bot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:bot": "nodemon src/bot.ts --max_old_space_size=2048",
    "start:bot:prod": "node src/bot.ts",
    "start:userbot:eternal": "forever ./src/userbot",
    "start:userbot:prod": "ts-node ./src/userbot",
    "start:server": "nodemon ./src/express.server.ts --max_old_space_size=2048",
    "start:server:prod": "ts-node ./src/express.server.ts",
    "start:server:cluster": "pm2 start ./src/express.server.ts -i 3 --watch || echo '*** Install PM2 globally'",
    "download-swindler": "node ./dataset/download-swindler-dataset.js && npm run optimize-swindler",
    "optimize-swindler": "node ./dataset/optimize-swindler-dataset.js positives && node ./dataset/optimize-swindler-dataset.js negatives",
    "prepare": "husky install",
    "test": "jest",
    "lint": "npm-run-all --parallel lint:js",
    "lint:branch-name": "branch-name-lint ./.branchlintrc.json",
    "lint:commit-name": "commitlint --edit",
    "track-time": "smile-track",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@grammyjs/menu": "^1.1.2",
    "@grammyjs/parse-mode": "^1.4.0",
    "@grammyjs/router": "^2.0.0",
    "@grammyjs/transformer-throttler": "^1.1.2",
    "@keyv/sqlite": "^3.6.1",
    "@mtproto/core": "6.2.0",
    "@tensorflow/tfjs": "^3.21.0",
    "@tensorflow/tfjs-node": "^3.21.1",
    "@types/fuzzyset": "^1.0.4",
    "aws-sdk": "^2.1230.0",
    "axios": "^1.0.0",
    "bottleneck": "^2.19.5",
    "contains-emoji": "^1.1.4",
    "cyrillic-to-translit-js": "^3.2.1",
    "diff": "^5.1.0",
    "eventsource": "^2.0.2",
    "express": "^4.18.1",
    "fuse.js": "^6.6.2",
    "fuzzyset": "^1.0.7",
    "google-auth-library": "^8.5.2",
    "googleapis": "^108.0.0",
    "grammy": "^1.11.2",
    "grapheme-splitter": "^1.0.4",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "keyv": "^4.5.0",
    "lodash.get": "^4.4.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "p-iteration": "^1.1.8",
    "queue-promise": "^2.2.1",
    "redis": "^4.3.1",
    "string-similarity": "^4.0.4",
    "typed-dotenv": "^10.0.2",
    "typegram": "^4.0.0",
    "ukrainian-ml-optimizer": "github:DrSmile444/ukrainian-ml-optimizer",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@swc/core": "^1.3.8",
    "@types/eventsource": "^1.1.9",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/string-similarity": "^4.0.0",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "branch-name-lint": "^2.1.1",
    "commitlint": "^17.1.2",
    "cors": "^2.8.5",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^44.0.1",
    "forever": "^4.0.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "queue": "^6.0.2",
    "smile-track": "^0.17.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "type-fest": "^3.0.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.8.4",
    "worker-farm": "^1.7.0"
  }
}
