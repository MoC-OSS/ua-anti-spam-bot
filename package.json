{
  "name": "ua-anti-spam-bot",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start:bot": "nodemon -L src/index.ts --max_old_space_size=2048",
    "start:bot:prod": "tsx src/index.ts",
    "start:bot:js:prod": "node dist/src/index.js",
    "start:userbot:eternal": "forever -c ts-node ./src/userbot/index.ts --transpile-only",
    "start:userbot:prod": "ts-node ./src/userbot/index.ts --transpile-only",
    "start:server": "nodemon ./src/express.server.ts --max_old_space_size=2048",
    "start:server:prod": "tsx ./src/express.server.ts",
    "start:server:js:prod": "node ./dist/src/express.server.js",
    "start:server:cluster": "(npx pm2 delete all || exit 0) && npx pm2 start ./src/express.server.ts -i max && npx pm2 monit || echo '*** Install PM2 globally'",
    "check-swindler:positives": "ts-node ./dataset/check-swindlers.ts positives",
    "check-swindler:negatives": "ts-node ./dataset/check-swindlers.ts negatives",
    "build": "FROM=src npm run build:folder && FROM=dataset npm run build:folder",
    "build:folder": "swc $FROM --copy-files --out-dir dist/$FROM --config-file .swcrc.json",
    "download-swindler": "ts-node dataset/download-swindler-dataset.ts && npm run optimize-swindler",
    "optimize-swindler": "ts-node dataset/optimize-swindler-dataset.ts positives && ts-node dataset/optimize-swindler-dataset.ts negatives",
    "optimize-obscene": "ts-node ./dataset/obscene-optimization.ts",
    "prepare": "husky install",
    "test": "jest",
    "health-check": "echo health-check",
    "lint": "npm-run-all --parallel lint:js lint:type-check test",
    "lint:branch-name": "branch-name-lint ./.branchlintrc.json",
    "lint:commit-name": "commitlint --edit",
    "track-time": "smile-track",
    "lint:js": "eslint . --cache --quiet",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@grammyjs/auto-thread": "github:grammyjs/auto-thread",
    "@grammyjs/menu": "^1.2.1",
    "@grammyjs/parse-mode": "^1.9.0",
    "@grammyjs/router": "^2.0.0",
    "@grammyjs/runner": "^1.0.4",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "@grammyjs/types": "^3.8.0",
    "@keyv/sqlite": "^3.6.7",
    "@mtproto/core": "6.2.0",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.4.16",
    "@tensorflow/tfjs": "^4.1.0",
    "@tensorflow/tfjs-node": "^4.18.0",
    "@types/fuzzyset": "^1.0.7",
    "@types/node": "^20.12.7",
    "aws-sdk": "^2.1601.0",
    "axios": "^1.6.8",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "cyrillic-to-translit-js": "^3.2.1",
    "deepmerge": "^4.3.1",
    "diff": "^5.2.0",
    "escape-html": "^1.0.3",
    "eventsource": "^2.0.2",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "fuse.js": "^6.6.2",
    "fuzzyset": "^1.0.7",
    "google-auth-library": "^8.9.0",
    "googleapis": "^109.0.1",
    "grammy": "^1.24.1",
    "grammy-guard": "^0.3.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lodash.get": "^4.4.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nsfwjs": "file:src/packages/nsfwjs-2.4.2.tgz",
    "p-iteration": "^1.1.8",
    "queue-promise": "^2.2.1",
    "redis": "^4.6.13",
    "sharp": "^0.31.3",
    "string-similarity": "^4.0.4",
    "tinyld": "^1.3.4",
    "typed-dotenv": "^10.0.2",
    "typegram": "^5.2.0",
    "ukrainian-ml-optimizer": "github:DrSmile444/ukrainian-ml-optimizer",
    "validator": "^13.11.0"
